<?xml version="1.0"?>
<extension key="namelessprogress" type="module">
  <file>namelessprogress</file>
  <name>Nameless Network Student Progress</name>
  <description>Provides features for advancing contacts through a sequence of sub-types, e.g., child, youth, adult, etc.</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>Allen Shaw</author>
    <email>allen@JoineryHQ.com</email>
  </maintainer>
  <requires>
    <ext>com.joineryhq.ageprogress</ext>
  </requires>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2020-02-02</releaseDate>
  <version>1.0</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>Developed for NamelessNetwork.org by Joinery</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Namelessprogress</namespace>
  </civix>
</extension>
